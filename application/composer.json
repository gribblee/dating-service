{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "name": "monsi/dating-service",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "src/Database.php",
      "src/Bootstrap.php",
      "src/Router.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^12.5"
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "vlucas/phpdotenv": "^5.6",
    "ext-igbinary": "*",
    "ext-pgsql": "*"
  },
  "scripts": {
    "post-autoload-dump": [
      "php bin/generate"
    ]
  }
}
